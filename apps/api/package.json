{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nest start --watch",
    "build": "nest build",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "db:generate": "prisma generate --schema=./src/infrastructure/database/prisma/schema.prisma",
    "db:push": "prisma db push --schema=./src/infrastructure/database/prisma/schema.prisma",
    "db:pull": "prisma db pull --schema=./src/infrastructure/database/prisma/schema.prisma",
    "db:migrate": "prisma migrate deploy --schema=./src/infrastructure/database/prisma/schema.prisma",
    "db:migrate:dev": "prisma migrate dev --schema=./src/infrastructure/database/prisma/schema.prisma",
    "db:migrate:reset": "prisma migrate reset --schema=./src/infrastructure/database/prisma/schema.prisma",
    "docker:up": "docker-compose -f ./src/infrastructure/docker/docker-compose.yaml up -d",
    "docker:down": "docker-compose -f ./src/infrastructure/docker/docker-compose.yaml down",
    "docker:logs": "docker-compose -f ./src/infrastructure/docker/docker-compose.yaml logs -f",
    "docker:ps": "docker-compose -f ./src/infrastructure/docker/docker-compose.yaml ps",
    "docker:restart": "docker-compose -f ./src/infrastructure/docker/docker-compose.yaml restart",
    "docker:build": "docker-compose -f ./src/infrastructure/docker/docker-compose.yaml build",
    "docker:pull": "docker-compose -f ./src/infrastructure/docker/docker-compose.yaml pull"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@pipu/api": "workspace:*",
    "@prisma/client": "^6.13.0",
    "@workos-inc/node": "^7.67.0",
    "amqplib": "^0.10.8",
    "jose": "^6.0.12",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.1.5",
    "passport-http-bearer": "^1.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@repo/eslint-config": "workspace:*",
    "@repo/jest-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/amqplib": "^0.10.7",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.13.10",
    "@types/node-jose": "^1.1.13",
    "@types/passport-http-bearer": "^1.0.41",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "prisma": "^6.13.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  }
}
